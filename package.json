      "name": "Satyendra Pandit"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "global": "^4.4.0",
    "nodemon": "^2.0.4",
    "routes": "^2.1.0",
    "xtend": "^4.0.2"
  },
  "devDependencies": {
    "function-bind": "^1.1.1",
    "run-browser": "^2.0.2",
    "tape": "^5.0.1"
  },
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.4.1"
  }
