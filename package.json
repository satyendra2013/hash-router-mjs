{
  "name": "hash-router-mjs",
  "version": "0.0.1",
  "description": "A lightweight javascript utility for building hash routing in your front-end application.",
  "main": "index.mjs",
  "scripts": {
    "test": "node --experimental-modules --es-module-specifier-resolution=node ./tests/index.mjs",
    "start": "node --experimental-modules --es-module-specifier-resolution=node ./index.mjs",
    "watch": "nodemon --experimental-modules --es-module-specifier-resolution=node -w ./index.mjs index.mjs"
  },
  "keywords": [
    "hash-router-js",
    "react-hash-routing",
    "experimental-mjs-routing-library"
  ],
